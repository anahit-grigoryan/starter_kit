{
  "name": "starter_kit",
  "version": "0.0.1",
  "dependencies": {
    "eslint-plugin-import": "^2.9.0",
    "eslint-watch": "^3.1.3",
    "express": "^4.16.3",
    "jsdom": "^7.0.2",
    "npm-run-all": "^4.1.2",
    "open": "0.0.5",
    "path": "^0.12.7"
  },
  "scripts": {
    "prestart": "babel-node buildScripts/src/server/prehook.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch start-mockapi",
    "security-check": "nsp check",
    "localtunnel": "lt --port 8000 --subdomain picsart",
    "open:src": "babel-node buildScripts/src/server/app.js",
    "lint": "esw buildScripts/src/server/*.js --color",
    "lint:watch": "npm run lint -- --watch",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "mocha --reporter progress buildScripts/src/test/testSetup.js \"buildScripts/src/test/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "mock-data-generate": "babel-node buildScripts/src/server/generateMockData.js",
    "prestart-mockapi": "npm run mock-data-generate",
    "start-mockapi": "json-server --watch buildScripts/src/client/api/db.json --port 3000"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "botbuilder": "^4.0.0-alpha2",
    "chai": "^4.1.2",
    "eslint": "^4.19.0",
    "fetch": "^1.1.0",
    "fs": "0.0.1-security",
    "grunt": "^1.0.2",
    "grunt-contrib-jshint": "^1.1.0",
    "json-schema-faker": "^0.5.0-rc13",
    "json-server": "^0.12.1",
    "localtunnel": "^1.8.3",
    "mocha": "^5.0.4",
    "nsp": "^3.2.1",
    "webpack": "^4.1.1",
    "webpack-dev-middleware": "^3.0.1",
    "whatwg-fetch": "^2.0.3"
  }
}
